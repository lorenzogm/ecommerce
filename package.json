{
  "name": "cms-contentful",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./src --ext .js,.jsx,.tsx,.ts",
    "setup": "node ./contentful/setup.js $CONTENTFUL_SPACE_ID $CONTENTFUL_MANAGEMENT_TOKEN",
    "types": "contentful-typescript-codegen --output src/types/generated/contentful.d.ts"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "yarn lint"
    ],
    "cypress/**/*.{js,ts,tsx}": [
      "yarn lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npx tsc"
    }
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "13.4.0",
    "@contentful/rich-text-types": "^14.1.1",
    "@types/classnames": "^2.2.10",
    "classnames": "^2.2.6",
    "contentful": "7.14.4",
    "date-fns": "2.10.0",
    "dotenv": "^8.2.0",
    "next": "^10.0.1",
    "prismic-javascript": "^3.0.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@types/gtag.js": "^0.0.3",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@valtech-ch/eslint-config-react": "0.0.9",
    "contentful-import": "^7.7.12",
    "contentful-typescript-codegen": "^3.2.1",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.9.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.9.5"
  },
  "license": "MIT"
}
